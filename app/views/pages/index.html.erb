<table class="table">
  <thead>
    <tr>
      <th scope="col">Page ID</th>
      <th scope="col">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @pages.each do |page| %>
      <tr>
        <td><%= page.id %></td>
        <td><%= link_to 'Show', [@survey, page] %></td>
        <td><%= link_to 'Destroy', survey_page_path(@survey, page),
        method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
